<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: rgb(49, 46, 43);" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.StatsController">
   <children>
      <TextField fx:id="statystykiSzukaj" layoutX="528.0" layoutY="50.0" promptText="Szukaj">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TableView fx:id="statystykiTabela" layoutX="184.0" layoutY="120.0" prefHeight="480.0" prefWidth="910.0" style="-fx-background-color: rgb(89, 86, 83);" styleClass="tekst" stylesheets="@style.css">
        <columns>
            <TableColumn prefWidth="75.0" text="Nazwa" />
          <TableColumn prefWidth="75.0" text="Ranking" />
          <TableColumn prefWidth="75.0" text="Zwycięstwa" />
            <TableColumn prefWidth="75.0" text="Porażki" />
            <TableColumn prefWidth="75.0" text="Remisy" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="statystykiWroc" layoutX="616.0" layoutY="618.0" mnemonicParsing="false" onAction="#onWrocButtonClick" text="Wróć" textFill="WHITE" />
   </children>
</AnchorPane>
